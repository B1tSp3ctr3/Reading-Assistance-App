{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./app/assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocalNetworkUsageDescription": "This app needs access to your local network to connect to local servers and devices.",
        "NSBonjourServices": ["_http._tcp", "_http._tcp"],
        "UIBackgroundModes": ["audio"],
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.anirudhrathore11333.frontend",
      "useNewArchitecture": false
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anirudhrathore11333.frontend",
      "useNewArchitecture": false
    },
    "web": {
      "favicon": "./app/assets/favicon.png"
    },
    "plugins": ["expo-router"],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a03088ae-f130-43fa-8b6f-96021d7ffd22"
      }
    }
  }
}
